<xs:schema xmlns:sql="urn:schemas-microsoft-com:mapping-schema"
	 xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 version="2.30"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="CRMAcresMessage">
		<xs:annotation>
			<xs:documentation>This general schema is used for the communication of messaging between the CRM project and IGT Patron Management.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Header"/>
				<xs:element ref="PlayerID" minOccurs="0"/>
				<xs:element name="Site" type="SiteType" minOccurs="0"/>
				<xs:element ref="Body" minOccurs="0"/>
				<xs:element ref="CreditID" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="SchemaVersion" type="xs:decimal"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Header">
		<xs:annotation>
			<xs:documentation>GSA/HITIS compatible message Header.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="FromURI" type="xs:anyURI" minOccurs="0"/>
				<xs:element name="ToURI" type="xs:anyURI" minOccurs="0"/>
				<xs:element name="ReplyToURI" type="xs:anyURI" minOccurs="0"/>
				<xs:element name="MessageID" type="SQLInt" minOccurs="0"/>
				<xs:element name="OriginalMessageID" type="SQLInt" minOccurs="0"/>
				<xs:element name="TimeStamp" type="xs:dateTime" minOccurs="0"/>
				<xs:element name="Token" type="xs:string" minOccurs="0"/>
				<xs:element name="PropertyID" type="xs:string" minOccurs="0"/>
				<xs:element name="Operation">
					<xs:complexType>
						<xs:attribute name="Operand">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Add"/>
									<xs:enumeration value="Update"/>
									<xs:enumeration value="Delete"/>
									<xs:enumeration value="Request"/>
									<xs:enumeration value="Information"/>
									<xs:enumeration value="Success"/>
									<xs:enumeration value="Validate"/>
									<xs:enumeration value="Lock"/>
									<xs:enumeration value="Create"/>
									<xs:enumeration value="Error"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Data">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="AbsUser"/>
									<xs:enumeration value="AddressType"/>
									<xs:enumeration value="Affiliation"/>
									<xs:enumeration value="CardPrint"/>
									<xs:enumeration value="CardsIssued"/>
									<xs:enumeration value="Comments"/>
									<xs:enumeration value="Comp"/>
									<xs:enumeration value="CompCenter"/>
									<xs:enumeration value="CompHistory"/>
									<xs:enumeration value="CompIssuance"/>
									<xs:enumeration value="CompList"/>
									<xs:enumeration value="CompListMessage"/>
									<xs:enumeration value="CompListType"/>
									<xs:enumeration value="CompRedemption"/>
									<xs:enumeration value="CountryInfo"/>
									<xs:enumeration value="CouponIssuance"/>
									<xs:enumeration value="CouponList"/>
									<xs:enumeration value="CouponListPlayer"/>
									<xs:enumeration value="CouponRedemption"/>
									<xs:enumeration value="CreditConditions"/>
									<xs:enumeration value="CreditInfo"/>
									<xs:enumeration value="EmailType"/>
									<xs:enumeration value="Error"/>
									<xs:enumeration value="FindPlayer"/>
									<xs:enumeration value="GamingProfile"/>
									<xs:enumeration value="General"/>
									<xs:enumeration value="Itinerary"/>
									<xs:enumeration value="Language"/>
									<xs:enumeration value="LinkInfo"/>
									<xs:enumeration value="LoginModify"/>
									<xs:enumeration value="Merge"/>
									<xs:enumeration value="Messages"/>
									<xs:enumeration value="PhoneType"/>
									<xs:enumeration value="PINInfo"/>
									<xs:enumeration value="PlayerActivity"/>
									<xs:enumeration value="PlayerBalanceAdjustment"/>
									<xs:enumeration value="PlayerBalances"/>
									<xs:enumeration value="PlayerSecondCompBalanceSet"/>
									<xs:enumeration value="PlayerXtraCreditBalances"/>
									<xs:enumeration value="PlayerCard"/>
									<xs:enumeration value="PlayerCompListMessage"/>
									<xs:enumeration value="PlayerFind"/>
									<xs:enumeration value="PlayerFind2"/>
									<xs:enumeration value="PlayerGroupAdd"/>
									<xs:enumeration value="PlayerGroupLookup"/>
									<xs:enumeration value="PlayerHost"/>
									<xs:enumeration value="PlayerIDByCreditID"/>
									<xs:enumeration value="PlayerImage"/>
									<xs:enumeration value="PlayerInterests"/>
									<xs:enumeration value="PlayerPIN"/>
									<xs:enumeration value="PlayerProfile"/>
									<xs:enumeration value="PlayerPromoEvent"/>
									<xs:enumeration value="PromoEventTicketIssuance"/>
									<xs:enumeration value="PlayerPromos"/>
									<xs:enumeration value="PlayerPromotions"/>
									<xs:enumeration value="PlayerRanking"/>
									<xs:enumeration value="PlayerReservation"/>
									<xs:enumeration value="PlayerReservations"/>
									<xs:enumeration value="PlayerRestrictions"/>
									<xs:enumeration value="PlayerStatus"/>
									<xs:enumeration value="PointsConversionRate"/>
									<xs:enumeration value="Promo"/>
									<xs:enumeration value="PromoWithFilter"/>
									<xs:enumeration value="PromotionAwardsIssuance"/>
									<xs:enumeration value="PromotionAwardsList"/>
									<xs:enumeration value="PromotionAwardsListPlayer"/>
									<xs:enumeration value="PromoAwardPlayerBalance"/>
									<xs:enumeration value="PromotionPlayerStatusUpdate"/>
									<xs:enumeration value="PromotionListSystem"/>
									<xs:enumeration value="PromotionListPlayer"/>
									<xs:enumeration value="PromoEvent"/>
									<xs:enumeration value="PromoEventsWithFilter"/>
									<xs:enumeration value="PromoEventBlock"/>
									<xs:enumeration value="PromoEventBlocksWithFilter"/>
									<xs:enumeration value="PromoMaster"/>
									<xs:enumeration value="PromoStatus"/>
									<xs:enumeration value="PromoType"/>
									<xs:enumeration value="RateRestriction"/>
									<xs:enumeration value="RateType"/>
									<xs:enumeration value="Rating"/>
									<xs:enumeration value="Reason"/>
									<xs:enumeration value="ReservationType"/>
									<xs:enumeration value="Restrictions"/>
									<xs:enumeration value="RoomRestriction"/>
									<xs:enumeration value="RoomType"/>
									<xs:enumeration value="SiteInfo"/>
									<xs:enumeration value="StopCodes"/>
									<xs:enumeration value="TripComment"/>
									<xs:enumeration value="TripInformation"/>
									<xs:enumeration value="TripSummary"/>
									<xs:enumeration value="UnMerge"/>
									<xs:enumeration value="UpdatePlayer"/>
									<xs:enumeration value="ZipCode"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="Command" type="xs:string"/>
						<xs:attribute name="WhereClause" type="xs:string"/>
						<xs:attribute name="MaxRecords" type="SQLInt"/>
						<xs:attribute name="TotalRecords" type="SQLInt"/>
						<xs:attribute name="Success">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Yes"/>
									<xs:enumeration value="No"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="PageNumber" type="xs:string"/>
						<xs:attribute name="MaxPages" type="xs:string"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="Information" type="xs:string" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="ImmediateResponseRequired" type="xs:boolean"/>
			<xs:attribute name="OriginalBodyRequested" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="PlayerID" type="xs:string" nillable="true"/>
	<xs:element name="CreditID" type="xs:string" nillable="true"/>
	<xs:element name="Site" type="SiteType" nillable="true"/>
	<xs:element name="Body">
		<xs:complexType>
			<xs:choice minOccurs="0">
				<xs:element sql:relation="AbsUser" name="AbsUser">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="UserID" type="SQLInt"/>
							<xs:element sql:field="LoginName" sql:relation="AbsUser" sql:datatype="varchar" name="LoginName">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="30"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="LastName" sql:relation="AbsUser" sql:datatype="varchar" name="LastName" nillable="true" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="50"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="FirstName" sql:relation="AbsUser" sql:datatype="varchar" name="FirstName">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="50"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="MiddleInitial" sql:relation="AbsUser" sql:datatype="char" name="MiddleInitial" nillable="true" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="Status" sql:relation="AbsUser" sql:datatype="char" name="Status">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="License" sql:relation="AbsUser" sql:datatype="varchar" name="License" nillable="true" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="16"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="SiteID" type="SQLSmallInt" default="1"/>
							<xs:element sql:field="XRef" sql:relation="AbsUser" sql:datatype="varchar" name="XRef" nillable="true" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="8"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="DisplayName" sql:relation="AbsUser" sql:datatype="varchar" name="DisplayName" nillable="true" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="15"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="JobTitle" sql:relation="AbsUser" sql:datatype="varchar" name="JobTitle" nillable="true" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="50"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="StaffID" type="SQLInt" nillable="true" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element sql:relation="AddressType" name="AddressType">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TypeID" type="SQLSmallInt"/>
							<xs:element sql:field="Description" sql:relation="AddressType" sql:datatype="varchar" name="Description">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="32"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="Status" sql:relation="AddressType" sql:datatype="char" name="Status" default="A">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="XRef" sql:relation="AddressType" sql:datatype="varchar" name="XRef" nillable="true" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="8"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Type" type="SQLTinyInt"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element sql:relation="Affiliation" name="Affiliation">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AffiliationID" type="SQLSmallInt"/>
							<xs:element sql:field="Description" sql:relation="Affiliation" sql:datatype="varchar" name="Description">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="32"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element sql:field="Status" sql:relation="Affiliation" sql:datatype="char" name="Status" default="A">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CardPrint">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CardPrintRequestType" type="SQLTinyInt"/>
							<xs:element name="Quantity" type="SQLSmallInt"/>
							<xs:element name="UserID" type="SQLInt"/>
							<xs:element name="Location" type="xs:string" minOccurs="0"/>
							<xs:element name="Workstation" type="xs:string" minOccurs="0"/>
							<xs:element name="Embosser" type="xs:string" minOccurs="0"/>
							<xs:element name="SerialNumber" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="256"/>
										<xs:minLength value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="RankingID" type="SQLSmallInt" minOccurs="0"/>
							<xs:element name="RankingName" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="16"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="CardID" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="20"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="FormattedCardID" type="xs:string" minOccurs="0"/>
							<xs:element name="PlayerName" type="Name" minOccurs="0"/>
							<xs:element name="CardStatus" type="xs:string" minOccurs="0"/>
							<xs:element name="ActivateDate" type="xs:dateTime" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Comments">
					<xs:complexType>
						<xs:choice>
							<xs:element name="Filter">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Sites" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Site" type="SiteType" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="CommentGroup" minOccurs="0">
											<xs:annotation>
												<xs:documentation>If not present, applies to all groups.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="CommentGroupID" type="SQLInt" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="CreatedBefore" type="xs:dateTime" minOccurs="0"/>
										<xs:element name="CreatedAfter" type="xs:dateTime" minOccurs="0"/>
										<xs:element name="HighPriority" type="xs:boolean" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Comment" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="IsHighPriority" type="xs:boolean" default="false"/>
										<xs:element name="Number" type="SQLInt" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Acres' Ordinal that keeps the comments in order.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="IsPrivate" type="xs:boolean" default="false" minOccurs="0"/>
										<xs:element name="IsGlobal" type="xs:boolean" default="false" minOccurs="0"/>
										<xs:element name="IsActive" type="xs:boolean" default="true" minOccurs="0"/>
										<xs:element name="Text" type="xs:string"/>
										<xs:element name="CreatedDate" type="xs:dateTime" minOccurs="0"/>
										<xs:element name="ExpirationDate" type="xs:dateTime" minOccurs="0"/>
										<xs:element name="EnteredBy">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="User" type="User"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Site" type="SiteType" minOccurs="0"/>
										<xs:element name="CommentGroup" minOccurs="0">
											<xs:annotation>
												<xs:documentation>If not present, applies to all groups.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="CommentGroupID" type="SQLInt" maxOccurs="unbounded"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="CardsIssued">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CardID" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="20"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="FormattedCardID" type="xs:string" minOccurs="0"/>
							<xs:element name="CardStatus" type="xs:string" minOccurs="0"/>
							<xs:element name="ActivateDate" type="xs:dateTime" minOccurs="0"/>
							<xs:element name="IssuedCount" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="SQLInt">
										<xs:minInclusive value="0"/>
										<xs:maxInclusive value="999"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Comp">
					<xs:annotation>
						<xs:documentation>Source attribute optional.  "CompDollars" means comp will be posted against the player's comp dollars.  "Points" means the comp will be posted against the player's points.  If no Source is specified, the comp will be posted against points.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Quantity" type="SQLInt" minOccurs="0"/>
							<xs:element name="PrintedComments" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="255"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="RestrictedComments" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="255"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="RedemptionNumber" type="SQLBigInt" minOccurs="0"/>
							<xs:element name="Status" type="xs:string" minOccurs="0"/>
							<xs:element name="IssuedDate" type="xs:dateTime" minOccurs="0"/>
							<xs:element name="EffectiveDate" type="xs:dateTime" minOccurs="0"/>
							<xs:element name="DollarValue" type="xs:decimal" minOccurs="0"/>
							<xs:element name="ItemNumber" minOccurs="0">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="SQLInt">
											<xs:attribute name="Description" type="xs:string"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="CurrentUser">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="User" type="User"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Authorizer">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="User" type="User"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Site" type="SiteType"/>
							<xs:element name="VoidReasonID" type="SQLInt" minOccurs="0"/>
							<xs:element name="VoidDescription" type="xs:string" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="Source" default="CompDollars">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="CompDollars"/>
									<xs:enumeration value="Points"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element sql:relation="CompCenter" name="CompCenter" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CenterID" type="SQLInt"/>
							<xs:element name="Description">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="32"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="ItemNumber" type="SQLInt" nillable="true" minOccurs="0"/>
							<xs:element sql:field="Status" sql:relation="CompCenter" sql:datatype="char" name="Status">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="SiteID" type="SQLSmallInt" default="1"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CompHistory">
					<xs:complexType>
						<xs:choice>
							<xs:element name="Filter" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="StartDate" type="xs:dateTime" minOccurs="0"/>
										<xs:element name="MaxNumber" type="SQLInt" minOccurs="0"/>
										<xs:element name="CompID" type="SQLInt" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Comps" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="RedemptionNumber" type="SQLBigInt"/>
										<xs:element name="Status">
											<xs:complexType>
												<xs:simpleContent>
